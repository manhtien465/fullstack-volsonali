{
  "kind": "collectionType",
  "collectionName": "htmls",
  "info": {
    "singularName": "html",
    "pluralName": "htmls",
    "displayName": "Html",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "desc": {
      "type": "text"
    },
    "url": {
      "type": "string"
    },
    "category_html": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category-html.category-html"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "screenshots": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "pros": {
      "type": "json"
    },
    "cons": {
      "type": "json"
    },
    "release_date": {
      "type": "date"
    },
    "developer": {
      "type": "string"
    },
    "plays": {
      "type": "string"
    },
    "rating": {
      "type": "decimal",
      "default": 4
    },
    "full_description": {
      "type": "blocks"
    },
    "tags": {
      "type": "json"
    },
    "is_display_home": {
      "type": "boolean",
      "default": false
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "game",
        "tool"
      ],
      "default": "game"
    }
  }
}
